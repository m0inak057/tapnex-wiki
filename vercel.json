{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)\\.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/volunteer-systems",
      "destination": "/EVENT-MANAGEMENT/volunteer-systems/index.html"
    },
    {
      "source": "/ticketing-platform",
      "destination": "/EVENT-MANAGEMENT/ticketing-platform/index.html"
    }
  ],
  "routes": [
    {
      "src": "/home.css",
      "dest": "/home.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/home.js",
      "dest": "/home.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/earth3d.js",
      "dest": "/earth3d.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/additional-ad-styles.css",
      "dest": "/additional-ad-styles.css",
      "headers": {
        "Content-Type": "text/css"
      }
    }
  ]
}