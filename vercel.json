{
  "buildCommand": null,
  "devCommand": null,
  "installCommand": null,
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "$comment": "Cache-Control strategy: HTML files use s-maxage=60 (CDN cache for 60s) with stale-while-revalidate=300 for better performance while ensuring content freshness. Static assets (.css, .js, .png) use max-age=31536000, immutable for optimal caching.",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=60, stale-while-revalidate=300"
        }
      ]
    },
    {
      "source": "/(.*)\\.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/volunteer-systems",
      "destination": "/EVENT-MANAGEMENT/volunteer-systems/index.html"
    },
    {
      "source": "/ticketing-platform",
      "destination": "/EVENT-MANAGEMENT/ticketing-platform/index.html"
    }
  ]
}